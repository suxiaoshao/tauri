{
  "name": "self-tools",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "pnpm run format && pnpm run oxlint && pnpm run tslint",
    "format": "prettier --config ./.prettierrc.js --write .",
    "oxlint": "oxlint --ignore-path ./.eslintignore --tsconfig=./tsconfig.json -c ./.oxlintrc.json --import-plugin --jsdoc-plugin --jest-plugin --vitest-plugin --jsx-a11y-plugin --nextjs-plugin --promise-plugin --node-plugin --security-plugin",
    "tslint": "tsc --build tsconfig.json",
    "build:tauri": "pnpm --filter './packages/*' run tauri build",
    "build:web": "pnpm --filter './packages/*' run build",
    "test": "vitest run -c ./config/test/vitest.config.ts",
    "test:watch": "vitest -c ./config/test/vitest.config.ts",
    "test:coverage": "vitest run --coverage -c ./config/test/vitest.config.ts",
    "commit": "git-cz",
    "prepare": "husky"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
