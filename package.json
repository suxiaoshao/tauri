{
  "name": "self-tools",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "pnpm run format && pnpm run oxlint && pnpm run tslint",
    "format": "prettier --config ./.prettierrc.js --write .",
    "oxlint": "oxlint --ignore-path ./.eslintignore --tsconfig=./tsconfig.json -c ./oxlintrc.json --import-plugin --jest-plugin --jsx-a11y-plugin --jsdoc-plugin --nextjs-plugin --vitest-plugin --promise-plugin",
    "tslint": "tsc --build tsconfig.json",
    "build:tauri": "pnpm --filter './packages/*' run tauri build",
    "build:web": "pnpm --filter './packages/*' run build",
    "test": "vitest run -c ./config/test/vitest.config.ts",
    "test:watch": "vitest -c ./config/test/vitest.config.ts",
    "test:coverage": "vitest run --coverage -c ./config/test/vitest.config.ts",
    "commit": "git-cz",
    "prepare": "husky"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.1.0",
    "@hookform/resolvers": "^3.9.0",
    "@mui/icons-material": "^6.1.1",
    "@mui/lab": "6.0.0-beta.10",
    "@mui/material": "^6.1.1",
    "@reduxjs/toolkit": "^2.2.7",
    "@tauri-apps/api": "1.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.2",
    "react-use": "^17.5.1",
    "ts-pattern": "^5.3.1",
    "valibot": "0.42.1",
    "zustand": "5.0.0-rc.2"
  },
  "devDependencies": {
    "@rsbuild/core": "1.0.5",
    "@rsbuild/plugin-react": "1.0.2",
    "@rsdoctor/rspack-plugin": "^0.4.4",
    "@tauri-apps/cli": "^1.6.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitest/coverage-v8": "^2.1.1",
    "code-inspector-plugin": "^0.16.1",
    "git-cz": "^4.9.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "oxlint": "^0.9.6",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
