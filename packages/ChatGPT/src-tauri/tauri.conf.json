{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "ChatGPT",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "close": true,
        "create": true,
        "setFocus": true,
        "show": true,
        "startDragging": true
      },
      "os": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
      "identifier": "chatgpt.sushao.top",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "ChatGPT",
        "transparent": true,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "label": "main",
        "decorations": false
      }
    ],
    "macOSPrivateApi": true
  }
}
